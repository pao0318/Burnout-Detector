{
    "manifest_version":2,
    "name": "Currenturl",
    "description": "Fetches current tab url",
    "version": 0.1,
    "author": "Ankit Oscar",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "URL time tracker."
    },
    "permissions": ["tabs", "activeTab", "http://127.0.0.1:5000/*", "storage"],
    "background": {
        "scripts" : ["background.js"],
        "persistent": false
    }
}